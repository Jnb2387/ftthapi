{
  "name": "noderest",
  "version": "1.3.0",
  "description": "Node version of my PHP services.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js"
  },
  "author": "Tobin Bradley",
  "license": "MIT",
  "dependencies": {
    "@mapbox/sphericalmercator": "^1.0.5",
    "datatables.net-editor-server": "^1.7.4",
    "dbgeo": "^1.1.0",
    "hapi": "^16.6.2",
    "hapi-bodyparser": "^2.0.2",
    "hapi-router": "^3.5.0",
    "hapi-swagger": "^8.0.0",
    "inert": "^4.2.1",
    "joi": "^13.0.1",
    "nodemon": "^1.18.3",
    "pg-promise": "^7.3.2",
    "squel": "^5.12.0",
    "vision": "^4.1.1",
    "vt-pbf": "^3.1.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      }
    },
    "plugins": [],
    "rules": {
      "indent": [
        2,
        4
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "quotes": [
        0,
        "single"
      ],
      "semi": [
        2,
        "always"
      ]
    }
  }
}
